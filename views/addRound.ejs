<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>AdminLTE 2 | Data Tables</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" href="../../bootstrap/css/bootstrap.min.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- DataTables -->
  <link rel="stylesheet" href="../../plugins/datatables/dataTables.bootstrap.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="../../dist/css/AdminLTE.min.css">
  <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" href="../../dist/css/skins/_all-skins.min.css">

  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.16/angular.min.js"></script>
  <script src="/javascript/RoundController.js"></script>

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body class="hold-transition skin-purple sidebar-mini" ng-app="RoundApp" ng-controller="roundController" ng-init="init('<%= caseId %>')">
<div class="wrapper">

  <header class="main-header">
    <!-- Logo -->
    <a href="../../index2.html" class="logo">
      <!-- mini logo for sidebar mini 50x50 pixels -->
      <span class="logo-mini"><b>C</b>C</span>
      <!-- logo for regular state and mobile devices -->
      <span class="logo-lg"><b>Case</b>Cove</span>
    </a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top">
      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </a>

      
    </nav>
  </header>
  <!-- Left side column. contains the logo and sidebar -->
  <aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
      
      <!-- sidebar menu: : style can be found in sidebar.less -->
      <ul class="sidebar-menu">
        
        <li class="treeview">
          <a href="/">
            <i class="fa fa-files-o"></i> <span>Cases</span>
            <!-- <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span -->
          </a>
          <!-- <ul class="treeview-menu">
            <li><a href="../../index.html"><i class="fa fa-circle-o"></i> Dashboard v1</a></li>
            <li><a href="../../index2.html"><i class="fa fa-circle-o"></i> Dashboard v2</a></li>
          </ul> -->
        </li>
        <li class="treeview">
          <a href="/addCase">
            <i class="fa fa-upload"></i>
            <span>Add Case</span>
            
          </a>
          
        </li>
        <li class="treeview">
          <a href="/addRound">
            <i class="fa fa-legal"></i>
            <span>Add Round</span>
            
          </a>
          
        </li>
        
        
        
      </ul>
    </section>
    <!-- /.sidebar -->
  </aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Add Round
      </h1>
      <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Add Round</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
            <div class="box-header with-border">
              <h3 class="box-title">Quick Example</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form role="form" ng-submit="submitRound()">
              <div class="box-body">
                <!-- Determines if you need to specify the case -->
                <div class="form-group" ng-show="!idProvided">
                  <label>Case</label>
                  <select class="form-control" ng-model="caseId">
                    <!-- <div ng-repeat="case in cases"> -->
                      <option ng-repeat="case in cases"> {{case._id}} </option>
                    <!-- </div> -->
                    
                    
                  </select>
                </div>

                <div class="form-group">
                  <label for="exampleInputEmail1">Date</label>
                  <input type="ext" class="form-control" id="exampleInputEmail1" ng-model="date" placeholder="Enter Date of Round">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">Gov</label>
                  <input type="ext" class="form-control" id="exampleInputEmail1" ng-model="gov" placeholder="Enter Gov Team">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">Opp</label>
                  <input type="ext" class="form-control" id="exampleInputEmail1" ng-model="opp" placeholder="Enter Opp Team">
                </div>
                
                <div class="form-group">
                  <label for="exampleInputEmail1">PM Speaks</label>
                  <input type="ext" class="form-control" id="exampleInputEmail1" placeholder="Enter PM Speaks" ng-model="pmSpeaks">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">LO Speaks</label>
                  <input type="ext" class="form-control" id="exampleInputEmail1" placeholder="Enter LO Speaks" ng-model="loSpeaks">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">MG Speaks</label>
                  <input type="ext" class="form-control" id="exampleInputEmail1" placeholder="Enter MG Speaks" ng-model="mgSpeaks">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">MO Speaks</label>
                  <input type="ext" class="form-control" id="exampleInputEmail1" placeholder="Enter MO Speaks" ng-model="moSpeaks">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">Winner</label>
                  <input type="ext" class="form-control" id="exampleInputEmail1" placeholder="Enter Description" ng-model="winner">
                </div>
                <!-- textarea -->
                <div class="form-group">
                  <label>Reason for Decision</label>
                  <textarea class="form-control" rows="5" placeholder="Enter ..." ng-model="reason"></textarea>
                </div>
                <!-- textarea -->
                <div class="form-group">
                  <label>Notes</label>
                  <textarea class="form-control" rows="5" placeholder="Enter ..." ng-model="notes"></textarea>
                </div>
        
              </div>
            

              <!-- /.box-body -->

              <div class="box-footer">
                <button type="submit" class="btn btn-primary">Submit</button>
              </div>
            </form>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  

 
  <!-- /.control-sidebar -->
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
<!-- ./wrapper -->

<!-- jQuery 2.2.3 -->
<script src="../../plugins/jQuery/jquery-2.2.3.min.js"></script>
<!-- Bootstrap 3.3.6 -->
<script src="../../bootstrap/js/bootstrap.min.js"></script>
<!-- DataTables -->
<script src="../../plugins/datatables/jquery.dataTables.min.js"></script>
<script src="../../plugins/datatables/dataTables.bootstrap.min.js"></script>
<!-- SlimScroll -->
<script src="../../plugins/slimScroll/jquery.slimscroll.min.js"></script>
<!-- FastClick -->
<script src="../../plugins/fastclick/fastclick.js"></script>
<!-- AdminLTE App -->
<script src="../../dist/js/app.min.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="../../dist/js/demo.js"></script>

<!-- page script -->
<script>
  $(function () {
    $("#example1").DataTable();
    $('#example2').DataTable({
      "paging": true,
      "lengthChange": false,
      "searching": false,
      "ordering": true,
      "info": true,
      "autoWidth": false
    });
  });
</script>
</body>
</html>
